{
  "name": "<%= themeName %>",
  "version": "1.0.0",
  "scripts": {
    "build": "gulp && patternlab build --config ./patternlab-config.json",
    "compile": "gulp compile && patternlab build --config ./patternlab-config.json",
    "watch": "gulp && concurrently \"npm:watch:*\"",
    "watch:patternlab": "patternlab serve --config ./patternlab-config.json",
    "watch:gulp": "gulp watch",
    "styleguide": "patternlab build --config ./patternlab-config.json",
    "compress": "gulp compress",
    "lint": "gulp lint",
    "clean": "gulp clean",
    "gulp": "gulp"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      [
        "@babel/preset-env"
      ]
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "del": "^4.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^5.0.0",
    "gulp-imagemin": "^6.0.0",
    "gulp-order": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5"
  }
}
